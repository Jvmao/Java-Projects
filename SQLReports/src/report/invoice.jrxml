	<queryString language="SQL">
		<![CDATA[SELECT
			c.id_client,c.name_client,c.lastname_client,c.country_client,
			c.city_client,c.tel_client,c.email_client,c.id_code,
			i.id_invoice,i.amount,i.vat,i.total,i.id_code
